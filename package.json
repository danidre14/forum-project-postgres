{
  "name": "forum-project-postgres",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "import-all": "npm install && cd client && npm install",
    "export": "git add . && git commit -m",
    "postexport": "git push && git push heroku master",
    "start": "node server.js",
    "devServer": "nodemon server.js",
    "devClient": "cd client && npm start",
    "devStart": "concurrently --kill-others-on-fail \"npm run devServer\" \"npm run devClient\"",
    "does-not-work-heroku-postbuild": "cd client && NPM_CONFIG_PRODUCTION=false npm i --only=dev && NPM_CONFIG_PRODUCTION=true npm i && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danidre14/forum-project-postgres.git"
  },
  "keywords": [
    "forum",
    "postgres",
    "knex"
  ],
  "author": "Danidre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danidre14/forum-project-postgres/issues"
  },
  "homepage": "https://github.com/danidre14/forum-project-postgres#readme",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "knex": "^0.20.9",
    "pg": "^7.18.1",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2"
  }
}
